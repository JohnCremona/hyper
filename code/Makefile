sextic: sextic.cc
	gcc -O4 sextic.cc -o sextic -g -lm -lstdc++ -lpari

sextic2: sextic2.cc
	gcc -O4 sextic2.cc -o sextic2 -g -lm -lstdc++ -lpari

xdeg4: xhyper.cc
	gcc -O4 -DDEGREE=4 xhyper.cc -o xdeg4 -g -lm -lstdc++ -lpari

hdeg4: hhyper.cc
	gcc -O4 -DDEGREE=4 hhyper.cc -o hdeg4 -g -lm -lstdc++ -lpari

xdeg6: xhyper.cc
	gcc -O4 -DDEGREE=6 xhyper.cc -o xdeg6 -g -lm -lstdc++ -lpari

xdeg8: xhyper.cc
	gcc -O4 -DDEGREE=8 xhyper.cc -o xdeg8 -g -lm -lstdc++ -lpari

xdeg10: xhyper.cc
	gcc -O4 -DDEGREE=10 xhyper.cc -o xdeg10 -g -lm -lstdc++ -lpari

negdef: negdef.cc
	gcc -O4 negdef.cc -o negdef -g -lm -lstdc++ -lpari
